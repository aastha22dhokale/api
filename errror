@NoArgsConstructor

@AllArgsConstructor

@Builder

public class CreatePostalAddressResponse {

    @JsonProperty("postalAddress")

    private PostalAddressResponse postalAddress;

    @JsonProperty("_links")

    private Map<String, Object> links;

}

